{
  "expo": {
    "name": "WaifuDraft",
    "slug": "WaifuDraft",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppIcon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.waifu.draft",
      "buildNumber": "0.0.1",
      "supportsTablet": true
    },
    "android": {
      "package": "com.waifu.draft",
      "versionCode": 1
    }, 
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }

}
